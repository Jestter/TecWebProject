@model IEnumerable<TecWeb.ControlAsistencia.Participante>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td style="vertical-align:middle">
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
    </tr>
}

</table>
