@model IEnumerable<TecWeb.ControlAsistencia.Participante>

<table class="table">
    <tr>
        <th>
            Profesor
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.ActionLink("Iniciar Prueba","Login","Usuario", new { id=item.ID , nombre=item.Nombre},null)
        </td>
    </tr>
}

</table>
